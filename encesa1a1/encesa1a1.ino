  digitalWrite(c7,LOW);
  digitalWrite(c8,HIGH);
  delay(150);
  digitalWrite(c8,LOW);
  digitalWrite(c9,HIGH);
  delay(150);
  digitalWrite(c9,LOW);
  digitalWrite(n1,LOW);
  digitalWrite(n2,HIGH);
  digitalWrite(c1,HIGH);
  delay(150);
  digitalWrite(c1,LOW);
  digitalWrite(c2,HIGH);
  delay(150);
  digitalWrite(c2,LOW);
  digitalWrite(c3,HIGH);
  delay(150);
